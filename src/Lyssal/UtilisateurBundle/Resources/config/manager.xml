<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="fos_user.user_manager.class">Lyssal\UtilisateurBundle\Manager\UtilisateurManager</parameter>
        <parameter key="fos_user.group_manager.class">Lyssal\UtilisateurBundle\Manager\UtilisateurGroupeManager</parameter>
        <parameter key="lyssal.utilisateur.manager.civilite.class">Lyssal\UtilisateurBundle\Manager\CiviliteManager</parameter>
    </parameters>

    <services>
        <service id="lyssal.utilisateur.manager.civilite" class="%lyssal.utilisateur.manager.civilite.class%">
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument>%lyssal.utilisateur.entity.civilite.class%</argument>
        </service>
    </services>

</container>
